        "outputId": "55781961-8ced-441b-94cf-9c2b9c51ce6e"
      },
      "source": [
        "#runs slow\n",
        "cvres=[]\n",
        "for num in tqdm(num_trees1):\n",
        "    rf = RandomForestClassifier(n_estimators = num, max_features='sqrt',max_depth=4,random_state=0)\n",
        "    cvclf = cross_validate(rf, X_scaled, y_train, cv=5, scoring='f1_weighted')\n",
        "    cvres.append(cvclf['test_score'].mean())"
      ],
      "execution_count": null,
      "outputs": [
